{
  "name": "tanstack-mock-server-fn",
  "version": "0.1.0",
  "description": "Mock server functions for TanStack Start server functions",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./vite": {
      "import": "./dist/esm/vite.js",
      "require": "./dist/cjs/vite.js",
      "types": "./dist/types/vite.d.ts"
    }
  },
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "tanstack",
    "mock",
    "server",
    "vite",
    "storybook"
  ],
  "author": "Eugen Eistrach",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EugenEistrach/tanstack-mock-server-fn"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "dependencies": {
    "@babel/generator": "^7.26.8",
    "@babel/parser": "^7.26.8",
    "@babel/traverse": "^7.26.8",
    "@babel/types": "^7.26.8"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/node": "^22.13.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:types",
    "build:esm": "tsc -p tsconfig.json --module ESNext --outDir dist/esm",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly --outDir dist/types",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  }
}
